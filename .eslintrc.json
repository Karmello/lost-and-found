{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "jasmine": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:jasmine/recommended"
    ],
    "globals": {
      "_": true,
      "$": true,
      "accounting": true,
      "fx": true,
      "angular": true,
      "before": true,
      "google":true,
      "MarkerClusterer": true,
      "io": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": [
      "jasmine"
    ],
    "rules": {
        "indent": ["error", 2, { "SwitchCase": 1 }],
        "linebreak-style": 0,
        "jasmine/no-spec-dupes": 0,
        "jasmine/new-line-between-declarations": 0,
        "jasmine/new-line-before-expect": 0,
        "quotes": [
            "error",
            "single"
        ],
        "no-console": 1,
        "no-empty": 1,
        "no-unused-vars": 1,
        "semi": [
            "error",
            "always"
        ]
    }
}